{
  "$schema": "https://schemas.florisboard.org/snygg/v2/stylesheet",
  "@defines": {
    "--background": "rgba(25,25,25,1)",
    "--surface": "rgba(255,140,0,1)",
    "--surface-variant": "rgba(255,110,0,1)",
    "--on-background": "rgba(255,255,255,1)",
    "--on-surface": "rgba(245,245,245,1)",
    "--border": "rgba(255,165,0,1)",
    "--shape": "rounded-corner(50%,50%,50%,50%)",
    "--shape-variant": "rounded-corner(12dp,12dp,12dp,12dp)"
  },
  "window": {
    "background": "var(--background)"
  },
  "key": {
    "background": "var(--surface)",
    "foreground": "var(--on-surface)",
    "font-size": "20sp",
    "shape": "var(--shape)",
    "shadow-elevation": "3dp",
    "border-color": "var(--border)",
    "border-width": "2dp"
  },
  "key[code=-201,-202]": { "font-size": "12sp" },
  "key[code=-205]": { "font-size": "10sp" },
  "key[code=10]": {
    "background": "var(--surface-variant)",
    "foreground": "var(--on-surface)",
    "border-color": "var(--border)"
  },
  "key[code=32]": {
    "background": "var(--surface)",
    "foreground": "var(--on-surface)",
    "font-size": "14sp",
    "border-color": "var(--border)"
  },
  "key:pressed": {
    "background": "var(--surface-variant)",
    "foreground": "var(--on-surface)",
    "border-color": "var(--on-background)"
  },
  "key-hint": {
    "background": "rgba(0,0,0,0)",
    "foreground": "rgba(255,245,220,0.8)",
    "font-size": "10sp",
    "padding": "1dp 1dp 1dp 1dp"
  },
  "key-popup-box": {
    "shape": "var(--shape)",
    "background": "var(--surface-variant)",
    "foreground": "var(--on-surface)",
    "border-color": "var(--border)",
    "border-width": "2dp"
  },
  "key-popup-element:focus": {
    "background": "var(--on-surface)",
    "foreground": "var(--surface)",
    "shape": "var(--shape)"
  },
  "clipboard-header": {
    "background": "rgba(0,0,0,0)",
    "foreground": "var(--on-background)",
    "font-size": "20sp",
    "border-width": "2dp",
    "border-color": "var(--border)"
  },
  "clipboard-item": {
    "background": "var(--surface)",
    "foreground": "var(--on-surface)",
    "font-size": "14sp",
    "shadow-elevation": "2dp",
    "border-color": "var(--border)",
    "border-width": "2dp",
    "margin": "6dp 6dp 6dp 6dp",
    "text-align": "left",
    "padding": "6dp 6dp 6dp 6dp",
    "shape": "var(--shape-variant)"
  },
  "clipboard-item-popup": {
    "background": "var(--surface)",
    "border-color": "var(--border)",
    "foreground": "var(--on-surface)",
    "margin": "6dp 6dp 6dp 6dp",
    "border-width": "2dp",
    "text-align": "center",
    "padding": "6dp 6dp 6dp 6dp",
    "font-size": "20sp",
    "shape": "var(--shape-variant)"
  },
  "clipboard-item-timestamp": {
    "foreground": "rgba(255,245,220,0.8)",
    "font-size": "10sp",
    "margin": "4dp 0dp 4dp 0dp",
    "text-align": "center"
  },
  "clipboard-item-actions": {
    "foreground": "var(--on-surface)",
    "margin": "6dp 6dp 6dp 6dp",
    "padding": "6dp 6dp 6dp 6dp"
  },
  "extracted-landscape-input-field": {
    "background": "rgba(0,0,0,0)",
    "foreground": "var(--on-background)",
    "font-size": "14sp",
    "shape": "rounded-corner(12dp,12dp,12dp,12dp)",
    "border-color": "var(--border)",
    "border-width": "1dp"
  },
  "emoji-key": {
    "background": "rgba(0,0,0,0)",
    "foreground": "var(--on-background)",
    "font-size": "20sp",
    "shape": "var(--shape)",
    "border-color": "var(--border)",
    "border-width": "2dp"
  },
  "emoji-key:pressed": {
    "background": "var(--surface)",
    "foreground": "var(--on-surface)",
    "border-color": "var(--on-background)"
  },
  "emoji-key-popup": {
    "background": "var(--background)",
    "foreground": "var(--on-surface)",
    "font-size": "20sp",
    "shape": "var(--shape)",
    "shadow-elevation": "2dp",
    "border-color": "var(--border)"
  },
  "keyboard": { "background": "var(--background)" },
  "system-nav-bar": {
    "background": "var(--surface)",
    "foreground": "var(--on-background)"
  }
}
